{
  "package": {
    "scripts": {
      "dev": "react-scripts start",
      "build": "react-scripts build",
      "test:unit": "react-scripts test",
      "test:unit:coverage": "react-scripts test --coverage",
      "prettier:check": "prettier --check './**/*.{ts,tsx,js,jsx}'",
      "prettier:write": "prettier --write './**/*.{ts,tsx,js,jsx}'",
      "eslint:check": "eslint --max-warnings 0 --ext .js,.ts,.tsx './'"
    },
    "dependencies": {
      "react": "18.2.0",
      "react-dom": "18.2.0"
    },
    "devDependencies": {
      "@testing-library/jest-dom": "5.16.5",
      "@testing-library/react": "13.4.0",
      "@testing-library/user-event": "14.4.3",
      "@types/jest": "29.0.2",
      "@types/node": "18.7.18",
      "@types/react": "18.0.20",
      "@types/react-dom": "18.0.6",
      "prettier": "2.7.1",
      "typescript": "4.8.3"
    },
    "eslintConfig": {
      "extends": ["react-app", "react-app/jest"]
    }
  }
